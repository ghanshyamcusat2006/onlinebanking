<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.onlinebanking.model">

	<class name="Users" table="users" batch-size="500">

		<id name="usersId" type="int" column="usersId"
			unsaved-value="0">
				<generator class="native" />
		</id>
		<property name="name" column="name" />
	</class>
</hibernate-mapping>
